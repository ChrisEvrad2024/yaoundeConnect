  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "prepare": "husky install",
    "test:db": "node src/utils/dbTest.js",
    "test:connection": "node src/utils/testConnection.js",
    "test:auth": "node src/utils/testAuth.js",
    "test:endpoints": "node src/utils/testEndpoints.js",
    "test:poi": "node src/utils/testPOI.js",
